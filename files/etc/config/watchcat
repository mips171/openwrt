config watchcat 'watchcat1'
	option forcedelay '10'
	option mode 'ping_reboot'
	option period '10m'
	option pingperiod '30s'
	option pinghosts 'google.com'

config watchcat 'watchcat2'
	option mode 'run_script'
	option period '1m'
	option pingperiod '30s'
	option pinghosts 'google.com'
	option script '/usr/bin/run_stateman.sh'

config watchcat 'watchcat3'
	option mode 'restart_iface'
	option mmifacename 'mobile'
	option unlockbands '1'
	option period '5m'
	option pingperiod '30s'
	option pinghosts 'google.com'

config watchcat 'watchcat4'
	option mode 'run_script'
	option period '1m'
	option pingperiod '30s'
	option pinghosts '172.16.0.1'
	option script '/usr/bin/recover_vpn.sh'
